import jarra.*
import personas.*
import marca.*
import carpas.*
test "testRequerimientos2" {
	const juan=new Alemanes()
	juan.leGustaLaMusicTrad(siLeGusta)
	const carpa1=new Carpas()
	const jarraFuerte=new Jarra()
	const palermo=new CervezaRubia()
	palermo.paisOrigen(belgica)
	palermo.lupudosXLitros(8)
	palermo.porcentAlcohol(5)
	jarraFuerte.capXLitros(5)
	jarraFuerte.esDeMarca(palermo)
	carpa1.jarra(jarraFuerte)
	carpa1.tieneMusica(conBandaMusica)
	
	assert.that(juan.quiereEntrar(carpa1))

	carpa1.limiteAdmitido(0)
	assert.notThat(carpa1.dejaIngresar(juan))
	assert.notThat(carpa1.puedeEntrar(juan))
	
}